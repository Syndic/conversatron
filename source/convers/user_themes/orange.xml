<castle:template name="index">{?standard_head}<table width="100%" cellspacing=0 cellpadding=0 border=0><tr><td background="/img/orangel.jpg" width=60><img src="/img/_.gif" width=60 height=1></td><td align=center width="100%"><br>{mainlogo}<br>{slogan}<br><br>{theme.PrintLinks()}<br>{?index_topics_start}{theme.PrintThreads()}{?index_topics_end}<hr width="80%">{?new_topic}{?copyright}</td><td background="/img/oranger.jpg" width=60><img src="/img/_.gif" width=60 height=1></td></tr></table></body></html></castle:template><castle:template name="index_date"><tr><td bgcolor="#ffff00" colspan="5" align="center"><table cellspacing=0 cellpadding=0 border=0 width=100%><tr><td align=left valign=top><img src="/img/o1.gif" height=6 width=6></td><td rowspan=2 width=100% align=center><font size="3" color="{bgcolor}"><b>{day}</b></font></td><td align=right valign=top><img src="/img/o2.gif" height=6 width=6></td></tr><tr><td align=left valign=bottom><img src="/img/o3.gif" height=6 width=6></td><td align=right valign=bottom><img src="/img/o4.gif" height=6 width=6></td></tr></table></td></tr></castle:template><castle:template name="index_thread"><tr><td>{star}</td><td><font size="3"><a href="convers.py?topic={row.id}&count={row.count}">{row.subject}</a></font></td><td><font size="2">Length: {row.count}</font></td><td><font size="2">{row.time}{user.usertype>1:	{row.followup=='y': &nbsp;&nbsp;(f)}	{row.archive=='y': &nbsp;&nbsp;<b>(A)</b>}}</font></td></tr></castle:template><castle:template name="index_topics_start"><table cellspacing=5 cellpadding=0 border=0><tr><td bgcolor="#ffff00" colspan="5" align="center"><table cellspacing=0 cellpadding=0 border=0 width=100%><tr><td align=left valign=top><img src="/img/o1.gif" height=6 width=6></td><td rowspan=2 width=100% align=center><font size="+1" color="{bgcolor}"><u><b>Current Topics</b></u></font></td><td align=right valign=top><img src="/img/o2.gif" height=6 width=6></td></tr><tr><td align=left valign=bottom><img src="/img/o3.gif" height=6 width=6></td><td align=right valign=bottom><img src="/img/o4.gif" height=6 width=6></td></tr></table></td></tr></castle:template><castle:template name="index_topics_end"></table></castle:template><castle:template name="topic">{?standard_head}<a href="{homepage}">{minilogo}</a><br><font size="+2">The Topic: <b><font color="#ffff00">{page.thread.subject}</font></b></font><br clear="all"><br>{theme.PrintThreadEntries()}<br>{archindex==None:{?thread_navigation}}{archindex!=None:{?archive_navigation}}<br>{page.ShowWriterStuff()}{?copyright}</body></html></castle:template><castle:template name="thread_navigation"><p align="center"><a href="convers.py?prev={page.thread.id}">{prevbutton}</a>{buttonseperator}<a href="{homepage}">{indexbutton}</a>{buttonseperator}<a href="convers.py?next={page.thread.id}">{nextbutton}</a></p></castle:template><castle:template name="archive_navigation"><br><p align="center"><font size=4><a href="/archive/archives{archindex}.html">Back to Archive Index</a></font></p></castle:template><!-- Basic Entry template, used for the non-table layout --><castle:template name="entry">{shadow:<font color="#666666">}{urlstart}<img src="{image}" width="80" height="100" border="0" align="{side}" hspace="5" alt="">{urlstop}<b>{name}</b> {buttonStr}<br>{text}<br clear="all">{userIDStr}<br><br>{shadow:</font>}</castle:template><!-- Left and Right table entry layouts --><castle:template name="entryl"><table cellpadding=0 cellspacing=0 border=0><tr><td bgcolor="{entrytitlebg}">&nbsp;</td><td bgcolor="{entrytitlebg}"><b><font color="#FFFFFF">{name}</font></b> {buttonStr}</td></tr><tr><td valign="top">{urlstart}<img src="{image}" width=80 height=100 border=0 alt="">{urlstop}</td><td valign="top" bgcolor="{color}"><table cellpadding=4><tr><td valign="top"><img src="/img/_.gif" width=100 height=1 alt=""><br>{text}</td></tr></table></td></tr></table>{userIDStr}<br></castle:template><castle:template name="entryr"><table cellpadding=0 cellspacing=0 border=0><tr><td bgcolor="{entrytitlebg}"><b><font color="#FFFFFF">{name}</font></b> {buttonStr}</td><td bgcolor="{entrytitlebg}">&nbsp;</td></tr><tr><td valign="top" bgcolor="{color}"><table cellpadding=4><tr><td valign="top"><img src="/img/_.gif" width=100 height=1 alt=""><br>{text}</td></tr></table></td><td valign="top">{urlstart}<img src="{image}" width=80 height=100 border=0 alt="">{urlstop}</td></tr></table>{userIDStr}<br></castle:template><castle:template name="new_topic"><p align="center"><b>Start a new topic</b></p><form method="post" action="newtopic.py">Enter a subject for your question:<input name="subject" type="text" size="32" maxlength="32"><br><br>Next, please elaborate on your question or discussion topic:<br><textarea name="body" rows="8" cols="60" wrap="soft" onkeydown="CtrlEnterSubmit()"></textarea><br>After completing a quick proofread, click here! <input type="submit" value="Post Topic"><br><br>{page.ShowFromAskee()}</form></castle:template><castle:template name="javascript"><script>function CtrlEnterSubmit()\{	if (this.event)	\{		e = this.event;		if ((e.keyCode==13) && (e.ctrlKey==true)) e.srcElement.form.submit();	\}\}</script></castle:template><castle:template name="copyright"><p align="right"><font color="#000000" size="-2" face="Helvetica, Arial">Images &copy; their respective owners. Text &copy; 1999-{currentyear} <font color="#ffff00"><b>The Conversatron.</b></font><br>For entertainment purposes only. Never eat glass.</font></p></castle:template><castle:template name="body"><body leftmargin=0 rightmargin=0 topmargin=0 bottommargin=0 marginwidth=0 marginheight=0 {bgcolor:bgcolor="{bgcolor}"} {bgimage != None:background="{bgimage}"} {bodytext:text="{bodytext}"} {bodylink:link="{bodylink}"} {bodyalink:alink="{bodyalink}"} {bodyvlink:vlink="{bodyvlink}"}></castle:template><castle:template name="standard_head"><html><head><title>{title}</title><meta name="description" content="{meta_description}"><meta name="keywords" content="{meta_keywords}">{?javascript}</head>{?body}</castle:template>